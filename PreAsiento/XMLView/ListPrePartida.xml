<view>
    <columns>
        <column name="id" display="none" order="100">
            <widget type="text" fieldname="idprepartida" />
        </column>
        <column name="subaccount" order="100">
            <widget type="text" fieldname="codsubcuenta" />
        </column>
        <column name="concept" order="110">
            <widget type="text" fieldname="concepto" />
        </column>
        <column name="debit" display="right" order="120">
            <widget type="text" fieldname="debe" />
        </column>
        <column name="credit" display="right" order="130">
            <widget type="text" fieldname="haber" />
        </column>
    </columns>
</view>
