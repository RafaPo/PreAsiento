<table>
    <column>
        <name>codsubcuenta</name>
        <type>character varying(50)</type>
        <null>NO</null>
    </column>
    <column>
        <name>concepto</name>
        <type>character varying(255)</type>
    </column>
    <column>
        <name>debe</name>
        <type>character varying(50)</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <column>
        <name>haber</name>
        <type>character varying(50)</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <column>
        <name>idpreasiento</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>idprepartida</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>orden</name>
        <type>integer</type>
        <null>NO</null>
        <default>0</default>
    </column>
    <constraint>
        <name>prepartidas_pkey</name>
        <type>PRIMARY KEY (idprepartida)</type>
    </constraint>
    <constraint>
        <name>ca_prepartidas_preasientos</name>
        <type>FOREIGN KEY (idpreasiento) REFERENCES preasientos (idpreasiento) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>